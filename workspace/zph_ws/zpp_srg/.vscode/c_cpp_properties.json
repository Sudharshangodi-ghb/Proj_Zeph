{
  "configurations": [
    {
      "name": "Zephyr STM32F446RE",
      "includePath": [
        "${workspaceFolder}/app/include",
        "${workspaceFolder}/modules/**",
        "${workspaceFolder}/build/zephyr/include",
        "${workspaceFolder}/../zephyr/include"
      ],
      "defines": [
        "CONFIG_CPP",
        "CONFIG_UART1",
        "CONFIG_UART2",
        "CONFIG_UART3"
      ],
      "compilerPath": "${workspaceFolder}/../Zephyr_sdk/arm-zephyr-eabi/bin/arm-zephyr-eabi-gcc.exe",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-arm"
    }
  ],
  "version": 4
}
